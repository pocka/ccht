{
  "name": "ccht",
  "description": "A simple command0line tool to crawl and test your website",
  "version": "0.1.1",
  "license": "MIT",
  "main": "cjs/index.js",
  "homepage": "https://pocka.github.io/ccht/",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "types": "esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pocka/ccht"
  },
  "keywords": [
    "crawling",
    "cli",
    "http",
    "puppeteer"
  ],
  "contributors": [
    {
      "name": "Shota Fuji",
      "url": "https://github.com/pocka"
    }
  ],
  "bin": {
    "ccht": "./bin/ccht.js"
  },
  "files": [
    "bin",
    "cjs",
    "esm"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^6.2.1",
    "node-fetch": "^2.6.7",
    "puppeteer-core": "^15.3.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^14.14.16",
    "@types/node-fetch": "^2.5.7",
    "@types/valid-url": "^1.0.3",
    "jest": "^28.1.2",
    "prettier": "^2.2.1",
    "puppeteer": "^15.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.build.json --outDir esm --module ESNext",
    "build:cjs": "tsc -p tsconfig.build.json --outDir cjs --module CommonJS",
    "build": "yarn build:esm && yarn build:cjs",
    "clean": "rimraf './{cjs,esm}/**/*.{js,d.ts}'",
    "test": "FORCE_COLOR=1 jest",
    "format": "prettier --write src",
    "check-format": "prettier --check src",
    "prepack": "yarn clean && yarn build"
  },
  "prettier": {}
}
